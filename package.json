{
  "name": "uteralia",
  "version": "1.0.0",
  "description": "Uteralia Under Construction Page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css css/icon-font.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlos-riquelme/uter.git"
  },
  "author": "Carlos Riquelme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlos-riquelme/uter/issues"
  },
  "homepage": "https://github.com/carlos-riquelme/uter#readme",
  "devDependencies": {
    "autoprefixer": "^9.2.1",
    "browserslist": "^4.3.1",
    "concat": "^1.0.3",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "live-server": "^1.2.0"
  }
}
